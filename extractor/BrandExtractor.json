{
  "ruleSets" : {
    "WordPressAssanka" : {
      "prefLabel" : "Extract brand info from a WP FT Blog that uses Assanka plug-in",
      "rules" : {
        "prefLabel" : { "select": ".top-title" , "extract" : "text"},
        "strapLine" : { "select": ".top-subtitle", "extract" : "text"},
        "descriptionXML" : { "select" : "div.content-tab:nth-child(3)", "extract" : "inner_html", "transformer" : "HtmlTransformer"},
        "description" : { "select" : "div.content-tab:nth-child(3)", "extract" : "text", "filter" : "([^\\n^\\r\\t]*)"},
        "_imageUrl" : { "select" : ".top-background-image", "extract" : "style", "filter" : ".*\\((.*)\\)"}
      }
    },
    "AboutLex" : {
      "prefLabel" : "Special rule for extracting for About Lex",
      "rules" : {
        "descriptionXML" : { "select" : "#storyContent > p:nth-child(1)", "extract" : "inner_html", "transformer" : "HtmlTransformer"},
        "description" : { "select" : "#storyContent > p:nth-child(1)", "extract" : "text", "filter" : "([^\\n^\\r\\t]*)"}
      }
    },
    "MarketsLive" : {
      "prefLabel" : "Special rule for extracting for MarketsLive",
      "rules" : {
        "descriptionXML" : { "select" : ".entry-content > p:nth-child(2)", "extract" : "inner_html", "transformer" : "HtmlTransformer"},
        "description" : { "select" : ".entry-content > p:nth-child(2)", "extract" : "text", "filter" : "([^\\n^\\r\\t]*)"}
      }
    }
  },
  "transformers" : {
    "HtmlTransformer" : {
      "url":"http://localhost:14080/content-transformer",
      "headers": [ "Content-Type:text/html"],
      "method": "POST"
    }
  },
